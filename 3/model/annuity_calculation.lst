1165   
1166  PARAMETER ANNUITYCG(CCC,GGG)               "Transforms investment in techn
      ologies into annual payment (fraction)";
1167   
1168  PARAMETER DEBT_SHARE_G(GGG)              "Share of debt for the investment
       of each generation technology (fraction)";
1169  DEBT_SHARE_G(GGG)$(GDATA(GGG,'GDKVARIABL') EQ 1)=0.8;
1170   
1171  PARAMETER INTEREST_RATE_G(GGG)           "Interest rate applied to the loa
      n of each generation technology (fraction)";
1172  INTEREST_RATE_G(GGG)$(GDATA(GGG,'GDKVARIABL') EQ 1)=0.06;
1173   
1174  PARAMETER PAYBACK_TIME_G(GGG)            "Payback time of the loan for gen
      eration technologies (years)";
1175  * Loan repayment assumption: lifetime of the technology if lifetime is hig
      her than 20 years, else 20 years
1176  PAYBACK_TIME_G(GGG)$(GDATA(GGG,'GDKVARIABL') EQ 1)=MIN(GDATA(GGG,'GDLIFETI
      ME'),20);
1177   
1178  PARAMETER ANNUITYCX(CCC)                 "Transforms investment in transmi
      ssion lines into annual payment (fraction)";
1179   
1180  SCALAR DEBT_SHARE_X                    "Share of debt for the investment o
      f transmission lines (fraction)";
1181  *Assumption: share of debt equal to 0
1182  DEBT_SHARE_X=0;
1183   
1184  SCALAR    INTEREST_RATE_X              "Interest rate applied to the loan 
      of transmission lines";
1185  *Assumption: interest rate equals socio economic discount rate
1186  INTEREST_RATE_X = DISCOUNTRATE;
1187   
1188  SCALAR    PAYBACK_TIME_X               "Payback time of the loan for trans
      mission lines (years)";
1189  *Assumption: payback time equals lifetime
1190  PAYBACK_TIME_X = LIFETIME_X;
