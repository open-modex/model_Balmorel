1162   
1163  PARAMETER ANNUITYCG(CCC,GGG)               "Transforms investment in techn
      ologies into annual payment (fraction)";
1164   
1165  PARAMETER DEBT_SHARE_G(GGG)              "Share of debt for the investment
       of each generation technology (fraction)";
1166  DEBT_SHARE_G(GGG)$(GDATA(GGG,'GDKVARIABL') EQ 1)=0.8;
1167   
1168  PARAMETER INTEREST_RATE_G(GGG)           "Interest rate applied to the loa
      n of each generation technology (fraction)";
1169  INTEREST_RATE_G(GGG)$(GDATA(GGG,'GDKVARIABL') EQ 1)=0.07;
1170   
1171  PARAMETER PAYBACK_TIME_G(GGG)            "Payback time of the loan for gen
      eration technologies (years)";
1172  * Loan repayment assumption: lifetime of the technology if lifetime is hig
      her than 20 years, else 20 years
1173  PAYBACK_TIME_G(GGG)$(GDATA(GGG,'GDKVARIABL') EQ 1)=GDATA(GGG,'GDLIFETIME')
      ;
1174   
1175  PARAMETER ANNUITYCX(CCC)                 "Transforms investment in transmi
      ssion lines into annual payment (fraction)";
1176   
1177  SCALAR DEBT_SHARE_X                    "Share of debt for the investment o
      f transmission lines (fraction)";
1178  *Assumption: share of debt equal to 0
1179  DEBT_SHARE_X=0;
1180   
1181  SCALAR    INTEREST_RATE_X              "Interest rate applied to the loan 
      of transmission lines";
1182  *Assumption: interest rate equals socio economic discount rate
1183  INTEREST_RATE_X = DISCOUNTRATE;
1184   
1185  SCALAR    PAYBACK_TIME_X               "Payback time of the loan for trans
      mission lines (years)";
1186  *Assumption: payback time equals lifetime
1187  PAYBACK_TIME_X = LIFETIME_X;
